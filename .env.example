# Application
NODE_ENV=development
PORT=3000
ORIGIN_URL=http://localhost:3000

# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/browser_pool?schema=public

# Authentication
JWT_SECRET=change-this-to-a-random-secret-key-in-production
API_KEY_PREFIX=bp_

# Rate Limiting (requests per minute)
RATE_LIMIT_FREE=5
RATE_LIMIT_PRO=30
RATE_LIMIT_ENTERPRISE=100

# Quotas (requests per month)
QUOTA_FREE=100
QUOTA_PRO=5000
QUOTA_ENTERPRISE=100000

# Dodo Payments (https://dodo.com)
DODO_API_KEY=your-dodo-api-key
DODO_WEBHOOK_SECRET=your-dodo-webhook-secret

# Browser Pool Configuration
BROWSER_POOL_SIZE=5
BROWSER_IDLE_TIMEOUT=300000
